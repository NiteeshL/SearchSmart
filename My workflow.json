        "contentType": "raw",
        "rawContentType": "application/json",
        "body": "=[\n    {\n        \"location_code\": 2840,\n        \"keywords\": {{ $json.keywordsData }},\n        \"date_from\": \"2025-01-01\",\n        \"search_partners\": true\n    }\n]",
        "options": {}
